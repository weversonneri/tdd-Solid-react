{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "esnext",
      "dom.Iterable"
    ],
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "rootDir": "src",
    "baseUrl": "src",
    "paths": {
      "@/*": [
        "*"
      ]
    },
    "allowJs": true,
    "resolveJsonModule": true,
  },
  "include": [
    "src"
  ],
}